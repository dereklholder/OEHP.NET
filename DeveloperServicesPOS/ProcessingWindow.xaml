<Window x:Class="DeveloperServicesPOS.ProcessingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DeveloperServicesPOS"
        mc:Ignorable="d"
        Title="Payment Transaction" Height="350" Width="300" ResizeMode="NoResize" ShowInTaskbar="False" HorizontalAlignment="Center" VerticalAlignment="Center" Icon="Images/RCM-Logo-16x16.ico" WindowStyle="None">
    <Grid>
        <WebBrowser x:Name="OEHPBrowser" HorizontalAlignment="Left" Height="295" Margin="10,15,0,0" VerticalAlignment="Top" Width="280" Loaded="OEHPBrowser_Loaded" LoadCompleted="OEHPBrowser_LoadCompleted"/>
        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="350" VerticalAlignment="Top" Width="300">
            <Button x:Name="OKButton" Content="Ok" VerticalAlignment="Top" Visibility="Visible" Click="OKButton_Click" Margin="112,317,111,0"/>
        </Border>

    </Grid>
</Window>
