        <TextBlock x:Name="noteText" HorizontalAlignment="Left" Margin="18,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="10.667"/>
        <TextBlock x:Name="fontFamily" HorizontalAlignment="Left" Margin="10,39,0,0" TextWrapping="Wrap" Text="Font Family" VerticalAlignment="Top"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="10,67,0,0" TextWrapping="Wrap" Text="Font Size" VerticalAlignment="Top"/>
        <TextBlock x:Name="textBlock_Copy" HorizontalAlignment="Left" Margin="10,95,0,0" TextWrapping="Wrap" Text="Background Color" VerticalAlignment="Top"/>
        <TextBlock x:Name="textBlock_Copy1" HorizontalAlignment="Left" Margin="10,123,0,0" TextWrapping="Wrap" Text="Button Color" VerticalAlignment="Top"/>
